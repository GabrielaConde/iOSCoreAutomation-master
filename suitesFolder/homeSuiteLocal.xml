<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="home suite"  verbose="1" parallel = "tests" thread-count="3">
    <test verbose="2" name="iPhone 15" >
        <parameter name="device" value="iPhone 15"></parameter>
        <parameter name="platformVersion" value="17"></parameter>
        <classes>
            <class name="testModules.home.fullFlowLambda"/>
        </classes>
    </test>
    <test verbose="2" name="iPhone 15 Plus" >
        <parameter name="device" value="iPhone 15 Plus"></parameter>
        <parameter name="platformVersion" value="17"></parameter>
        <classes>
            <class name="testModules.home.fullFlowLambda"/>
        </classes>
    </test>
    <test verbose="2" name="iPhone 15 Pro" >
        <parameter name="device" value="iPhone 15 Pro"></parameter>
        <parameter name="platformVersion" value="17"></parameter>
        <classes>
            <class name="testModules.home.fullFlowLambda"/>
        </classes>
    </test>
</suite>
