<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="newsAndShoppingSuite" verbose="1" parallel="tests" thread-count="2">
    <test verbose="2" name="iPhone15 Pro" >
        <parameter name="device" value="iPhone 15 Pro"></parameter>
        <parameter name="platformVersion" value="17"></parameter>
